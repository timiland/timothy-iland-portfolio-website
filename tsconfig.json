{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@pages/*": [
        "pages/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@atoms/*": [
        "components/atoms/*"
      ],
      "@molecules/*": [
        "components/3D/*"
      ],
      "@organisms/*": [
        "components/organisms/*"
      ],
      "@templates/*": [
        "components/templates/*"
      ],
      "@services/*": [
        "services/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@stores/*": [
        "stores/*"
      ]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
